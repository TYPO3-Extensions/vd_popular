<T3DataStructure>
    <meta>
        <langDisable>1</langDisable>
    </meta>
    <sheets>
        <sDEF>
            <ROOT>
                <TCEforms>
                    <sheetTitle>LLL:EXT:vd_popular/locallang_db.xml:vd_popular.flexform_pi2.s_main</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
				    <excludedPages>
                        <TCEforms>
                            <exclude>1</exclude>
                            <label>LLL:EXT:vd_popular/locallang_db.xml:vd_popular.flexform_pi2.excludedPages</label>
                            <config>
                                <type>group</type>
                                <internal_type>db</internal_type>
                                <allowed>pages</allowed>
                                <size>8</size>
                                <maxitems>50</maxitems>
                                <minitems>0</minitems>
                                <show_thumbs>1</show_thumbs>
                            </config>
                        </TCEforms>
                    </excludedPages>
					<pagesToShow>
						<TCEforms>
						<label>LLL:EXT:vd_popular/locallang_db.xml:vd_popular.flexform_pi2.pagesToShow</label>
						<config>
							<type>input</type>
							<eval>num</eval>
							<size>5</size>
							<max>3</max>
							<range>
								<lower>0</lower>
								<upper>100</upper>
							</range>
						</config>
						</TCEforms>
					</pagesToShow>
					<minVisits>
						<TCEforms>
						<label>LLL:EXT:vd_popular/locallang_db.xml:vd_popular.flexform_pi2.minVisits</label>
						<config>
							<type>input</type>
							<eval>num</eval>
							<size>5</size>
						</config>
						</TCEforms>
					</minVisits>
				</el>
            </ROOT>
        </sDEF>
    </sheets>
</T3DataStructure>
